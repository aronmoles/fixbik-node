{
  "name": "magicshopper-node",
  "version": "1.0.0",
  "main": "src/app/index.ts",
  "author": "aaron",
  "description": "Description of project",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=prod ts-node -r tsconfig-paths/register src/app/start.ts",
    "dev": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register src/app/start.ts",
    "lint": "node_modules/.bin/eslint 'src/**/*' --fix",
    "build": "yarn build:clean && yarn build:tsc && yarn build:di",
    "build:tsc": "node_modules/.bin/tsc -p tsconfig.prod.json",
    "build:di": "node_modules/.bin/copy 'src/**/*.{json,yaml,html,png}' build/src",
    "build:clean": "rm -r build; exit 0",
    "test": "yarn test:unit && yarn test:features",
    "test:unit": "NODE_ENV=test jest --coverage",
    "test:features": "NODE_ENV=test cucumber-js -p default",
    "migration:generate": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:generate --config ./orm.config.json --name",
    "migration:run": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:run --config ./orm.config.json",
    "migration:revert": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:revert --config ./orm.config.json"
  },
  "dependencies": {
    "amqp-ts": "^1.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.1",
    "glob": "^7.2.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.mergewith": "^4.6.2",
    "mongodb": "^4.2.2",
    "mysql": "^2.18.1",
    "openapi3-ts": "^2.0.1",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.0.0-rc.1",
    "@types/cucumber": "^7.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-promise-router": "^3.0.0",
    "@types/faker": "^5.5.9",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash.mergewith": "^4.6.6",
    "@types/node": "^16.11.12",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "copy": "^0.3.2",
    "eslint": "^8.5.0",
    "eslint-plugin-yaml": "^0.5.0",
    "faker": "^5.5.3",
    "jest": "^27.4.5",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.3"
  }
}
